{
  "name": "wp-cookie-logic-blocks",
  "version": "1.0.1",
  "description": "Track page visit counts via cookies and control block visibility based on visit thresholds.",
  "main": "build/index.js",
  "scripts": {
    "build": "wp-scripts build src/index.js src/frontend.js && npm run zip",
    "build:assets": "wp-scripts build src/index.js src/frontend.js",
    "zip": "zip -r wp-cookie-logic-blocks.zip . -x '*.git*' 'node_modules/*' 'src/*' '.DS_Store' '*.zip' 'package-lock.json'",
    "start": "wp-scripts start src/index.js src/frontend.js",
    "format": "wp-scripts format",
    "lint:js": "wp-scripts lint-js",
    "packages-update": "wp-scripts packages-update"
  },
  "keywords": [
    "wordpress",
    "gutenberg",
    "blocks",
    "cookie",
    "visibility"
  ],
  "author": "Michael Lajlev",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@wordpress/scripts": "^27.0.0"
  },
  "dependencies": {
    "@wordpress/block-editor": "^13.0.0",
    "@wordpress/blocks": "^13.0.0",
    "@wordpress/components": "^28.0.0",
    "@wordpress/element": "^6.0.0",
    "@wordpress/i18n": "^5.0.0"
  }
}
